        "@types/node": "20.10.0",
        "@typescript-eslint/eslint-plugin": "6.12.0",
        "@typescript-eslint/parser": "6.12.0",
        "@vitejs/plugin-vue": "4.5.0",
        "browserslist": "4.22.1",
        "confusing-browser-globals": "1.0.11",
        "eslint": "8.54.0",
